{
  "name": "lw-exam",
  "version": "0.1.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=20"
  },
  "types": "index.d.ts",
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && ts-node server.ts",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "ts-node-dev server.ts",
    "knex": "knex"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/env": "^4.2.0",
    "@fastify/sensible": "^5.0.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "knex": "^2.4.2",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.4"
  }
}
